<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Produce_Activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/broulims_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:text="Produce"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/produce1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/broulims_name"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="45dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/produce1"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="39dp"
        app:layout_constraintEnd_toStartOf="@+id/produce5"
        app:layout_constraintStart_toEndOf="@+id/produce"
        app:layout_constraintTop_toBottomOf="@+id/produce2"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="59dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/produce3"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="59dp"
        android:layout_marginBottom="53dp"
        app:layout_constraintBottom_toTopOf="@+id/produce9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/produce5"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="53dp"
        app:layout_constraintBottom_toTopOf="@+id/produce9"
        app:layout_constraintEnd_toStartOf="@+id/produce6"
        app:layout_constraintStart_toEndOf="@+id/produce8"
        app:layout_constraintTop_toBottomOf="@+id/produce4"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="45dp"
        android:layout_marginBottom="53dp"
        app:layout_constraintBottom_toTopOf="@+id/produce9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/produce"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="169dp"
        android:layout_marginEnd="170dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="39dp"
        app:layout_constraintEnd_toStartOf="@+id/produce3"
        app:layout_constraintStart_toEndOf="@+id/produce1"
        app:layout_constraintTop_toBottomOf="@+id/broulims_name"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/produce3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="59dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/broulims_name"
        app:srcCompat="@mipmap/ic_launcher" />
</android.support.constraint.ConstraintLayout>